---
#
# Ansible managed
#
exclude_paths:
  - ./molecule/default/prepare.yml
  - ./molecule/default/converge.yml
  - ./molecule/default/verify.yml
  - ./molecule/default/collections.yml
  - ./molecule/local/prepare.yml
  - ./molecule/local/converge.yml
  - ./molecule/local/verify.yml
  - ./molecule/local/collections.yml
  - ./.tox
  - ./.cache
  - ./.github
  - ./requirements.yml

warn_list:  # or 'skip_list' to silence them completely
  - fqcn[action-core]
  - yaml[comments-indentation]  # Violations reported by yamllint.
